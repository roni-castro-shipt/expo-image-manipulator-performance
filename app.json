{
  "expo": {
    "name": "MemoryProblem",
    "slug": "MemoryProblem",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "memoryproblem",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.MemoryProblem"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": ["android.permission.CAMERA"],
      "package": "com.anonymous.MemoryProblem"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "This app requires access to your photos to upload photos.",
          "cameraPermission": "This app requires access to your camera to upload photos."
        }
      ],
      [
        "react-native-permissions",
        {
          "iosPermissions": ["Camera", "PhotoLibrary"]
        }
      ]
    ]
  }
}
